{
  "name": "secneo-gadget",
  "author": {
    "email": "diff@protonmail.com",
    "name": "Tim Strazzere"
  },
  "version": "1.0.0",
  "description": "Gadgets for unpacking secneo protected appliciations",
  "private": true,
  "main": "build/index.cjs",
  "type": "module",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc package.json tsconfig.json ./research/jdwp/*.ts ./src/*.ts ./agent/*.ts --write",
    "prepare": "npm run build",
    "build": "frida-compile agent/index.ts -o build/_agent.js -c && tsc",
    "watch": "frida-compile agent/index.ts -o build/_agent.js -w",
    "lint": "npx @biomejs/biome check --write ./src",
    "start": "npm run prepare && node build/index.js",
    "spawn": "frida -U -f dji.go.v5 -l build/_agent.js",
    "spawn-pilot": "frida -U -f com.dji.industry.pilot -l build/_agent.js",
    "combine": "tsc && node build/combine.js"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/cli-progress": "^3.11.0",
    "@types/frida-gum": "^18.5.1",
    "@types/node": "^22.10.7",
    "frida-compile": "^16.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cli-progress": "^3.11.2",
    "frida": "^16.3.3",
    "frida-fs": "^5.2.2"
  }
}
